@model Microsoft.AspNetCore.Identity.IdentityRole

@{
    ViewData["Title"] = "CreateRole";
}



<h1>Create Role</h1>


<form asp-action="CreateRole">
    <div class="form-group">
        <label class="form-label" >Role Name:</label>
        <input class="form-control" type="text" name="roleName" ><br><br>
    </div>

    <div>
        <input class="btn btn-sm btn-warning" type="submit" value="Create Role">

    </div>
</form>

